<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context=".push.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:id="@+id/ididaaaaan"
        android:layout_height="wrap_content"/>
    <EditText
        android:layout_width="match_parent"
        android:id="@+id/idjudul"
        android:hint="juudul"
        android:visibility="invisible"
        android:layout_height="wrap_content"/>
    <EditText
        android:layout_width="match_parent"
        android:id="@+id/idpesan"
        android:visibility="invisible"
        android:hint="pesan"
        android:layout_height="wrap_content"/>
    <EditText
        android:layout_width="match_parent"
        android:id="@+id/idtoken"
android:visibility="invisible"
        android:hint="token"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:id="@+id/idsimpan"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="Pendaftaran Anggota Baru"
        android:textColor="@color/putiih"
        android:layout_height="50dp"/>



</LinearLayout>